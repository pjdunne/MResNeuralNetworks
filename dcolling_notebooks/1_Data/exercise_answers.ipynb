   "outputs": [],
   "source": [
    "# We might also want to try an exponential fit\n",
    "\n",
    "def ff(x,par):\n",
    "    a, b, c= par[0], par[1], par[2]\n",
    "    return b*np.exp(-(x-a)/c)\n",
    "\n",
    "param_1=[90, 2000, 10]\n",
    "range_1=[0,-1]\n",
    "\n",
    "fit(ff, param_1, range_1)\n",
    "\n",
    "# so now definitely looking like something between 120 and 130\n"
   ]
