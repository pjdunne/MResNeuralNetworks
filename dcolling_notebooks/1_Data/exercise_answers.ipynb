   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Alternatively, we could simply move to puting in a signal term as well\n",
    "# We will model the signal as a Gaussian\n",
    "\n",
    "def gaus(x,amp, offset, sigma):\n",
    "    return amp*np.exp(-(x-offset)**2/(2*sigma**2))\n",
    "\n",
    "def ff_new(x, par):\n",
    "    a, b, c = par[0], par[1], par[2]\n",
    "    amp,offset,sigma = par[3], par[4], par[5]\n",
    "    #Only add signal in the signal region as it may be distorted by other data\n",
    "    return b*np.exp(-(x-a)/c) + gaus(x,amp,offset,sigma)\n",
    "\n",
    "param_2=[90, 3000,10, 100, 125,2]\n",
    "range_2=[25, 100]\n",
    "\n",
    "fit(ff_new, param_2, range_2)\n",
    "\n",
    "#Visualy, this seems to accurately fit the data. \n",
    "#Feel free to perform greater analysis to check the goodness of fit"
