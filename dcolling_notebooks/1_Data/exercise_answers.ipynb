    "# c. using the accept/reject method \n",
    "\n",
    "x=[]\n",
    "\n",
    "for i in range(100000):\n",
    "    xr=npr.uniform(low=0,high=np.pi)\n",
    "    yr=npr.uniform()\n",
    "    if np.sin(xr) > yr :\n",
    "        x.append(xr)\n",
    "\n",
    "pl.hist(x,bins=100)"
