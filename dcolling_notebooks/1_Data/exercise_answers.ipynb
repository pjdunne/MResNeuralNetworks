   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# a. inverting the analytic integral\n",
    "\n",
    "import numpy as np\n",
    "import scipy as sp\n",
    "import pylab as pl\n",
    "import numpy.random as npr\n",
    "import scipy.interpolate as spi\n",
    "\n",
    "u=npr.uniform(size=100000,low=-1, high=1)\n",
    "x=np.arccos(u)\n",
    "\n",
    "pl.hist(x,bins=100);\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#b. integrating numerically\n",
    "\n",
    "x=np.arange(0,np.pi,0.01)\n",
    "y=np.sin(x)\n",
    "ycum=y.cumsum()\n",
    "ycum=ycum/ycum[-1]\n",
    "\n",
    "\n",
    "pl.subplot(2,1,1)\n",
    "pl.plot(ycum,x)\n",
    "\n",
    "\n",
    "f= spi.CubicSpline(ycum,x)\n",
    "\n",
    "yr=npr.uniform(size=1000000)\n",
    "\n",
    "xr=f(yr)\n",
    "\n",
    "pl.subplot(2,1,2)\n",
    "pl.hist(xr,bins=200)\n",
    "\n",
    "pl.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
